INSERT INTO zzz_content (cid, c_title, c_lid, c_sid, c_brand, c_type, c_gid, c_exc, c_title2, c_color, c_link, c_tag, c_content, c_onoff, c_order, isoutlink, istop, isgood, ispic, isoffer, issell, ishtml, c_visits, c_star, c_addtime, c_edittime, c_picsurl, c_picsname, c_pic, c_downurl, c_downname, c_pagename, c_pagetitle, c_pagekey, c_pagedesc, zauthor, zsource, zprice, zvprice) VALUES (1, '后台能否默认全屏打开？', 1, 6, NULL, 'news', 0, NULL, NULL, '#000000', NULL, NULL, '&lt;h2 data-line=&quot;0&quot; class=&quot;line&quot; style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; padding: 0px 0px 0.3em; font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; line-height: 1.7em; margin: 0px 0px 14px; font-weight: 200; font-size: 1.75em; border-bottom-width: 1px; border-bottom-style: solid; border-bottom-color: rgb(238, 238, 238); color: rgba(0, 0, 0, 0.870588); white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;解决方法&lt;/h2&gt;&lt;p style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; margin-top: 0px; margin-bottom: 14px; line-height: 27.132px; padding: 0px; color: rgba(0, 0, 0, 0.870588); font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;路径：\admin\js\adminjs.js&lt;p style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;大约第4行&lt;/p&gt;&lt;pre style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; overflow: auto; font-family: Consolas, &#39;Liberation Mono&#39;, Menlo, Courier, monospace; font-size: 13.566px; margin-top: 0px; margin-bottom: 14px; padding: 16px; line-height: 1.45; border: 0px; border-radius: 3px; color: rgba(0, 0, 0, 0.870588); background-color: rgb(247, 247, 247);&quot;&gt;function&nbsp;opennew(url){layer.open({type:2,shade:0.8,&nbsp;maxmin:&nbsp;true,&nbsp;area:[&#39;800px&#39;,&#39;95&#037;&#39;],content:url});}&lt;/pre&gt;&lt;p style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; margin-top: 0px; margin-bottom: 14px; line-height: 27.132px; padding: 0px; color: rgba(0, 0, 0, 0.870588); font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;改为&lt;/p&gt;&lt;pre style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; overflow: auto; font-family: Consolas, &#39;Liberation Mono&#39;, Menlo, Courier, monospace; font-size: 13.566px; margin-top: 0px; margin-bottom: 0px; padding: 16px; line-height: 1.45; border: 0px; border-radius: 3px; color: rgba(0, 0, 0, 0.870588); background-color: rgb(247, 247, 247);&quot;&gt;function&nbsp;opennew(url){layer.open({type:2,shade:0.8,&nbsp;maxmin:&nbsp;true,&nbsp;area:[&#39;100&#037;&#39;,&#39;100&#037;&#39;],content:url});}&lt;/pre&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;', 1, 8, NULL, 0, 0, 0, 0, 0, 0, 1, 0, '2017-10-28 18:13:01', NULL, NULL, NULL, NULL, NULL, NULL, NULL, '后台能否默认全屏打开？', '后台能否默认全屏打开？', '解决方法路径：\admin\js\adminjs.js大约第4行function&nbsp;opennew(url){layer.open({type:2,shade:0.8,&nbsp;maxmin:&nbsp;true,&nbsp;area:[&#39;800px&#39;,&#39;95%&#39;],content:url});}改为function&nbsp;opennew(url){layer.open({type:2,shade:0.8,&nbsp;maxmin:&…', NULL, NULL, 1, 1);
INSERT INTO zzz_content (cid, c_title, c_lid, c_sid, c_brand, c_type, c_gid, c_exc, c_title2, c_color, c_link, c_tag, c_content, c_onoff, c_order, isoutlink, istop, isgood, ispic, isoffer, issell, ishtml, c_visits, c_star, c_addtime, c_edittime, c_picsurl, c_picsname, c_pic, c_downurl, c_downname, c_pagename, c_pagetitle, c_pagekey, c_pagedesc, zauthor, zsource, zprice, zvprice) VALUES (2, '有时候内容会出现乱码？', 1, 6, NULL, 'news', 0, NULL, NULL, '#000000', NULL, NULL, '&lt;h2 data-line=&quot;0&quot; class=&quot;line&quot; style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; padding: 0px 0px 0.3em; font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; line-height: 1.7em; margin: 0px 0px 14px; font-weight: 200; font-size: 1.75em; border-bottom-width: 1px; border-bottom-style: solid; border-bottom-color: rgb(238, 238, 238); color: rgba(0, 0, 0, 0.870588); white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;问题描述&lt;/h2&gt;&lt;p style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; margin-top: 0px; margin-bottom: 14px; line-height: 27.132px; padding: 0px; color: rgba(0, 0, 0, 0.870588); font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;zzzcms全站都是utf-8格式，部分浏览器特别是IE，有些默认使用gbk编码打开页面，提交数据就可能造成数据乱码。&lt;/p&gt;&lt;h2 data-line=&quot;4&quot; class=&quot;line&quot; style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; padding: 0px 0px 0.3em; font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; line-height: 1.7em; margin: 0px 0px 14px; font-weight: 200; font-size: 1.75em; border-bottom-width: 1px; border-bottom-style: solid; border-bottom-color: rgb(238, 238, 238); color: rgba(0, 0, 0, 0.870588); white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&lt;a id=&quot;_4&quot; style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; color: rgb(65, 131, 196); margin: 0px; padding: 0px; background: transparent;&quot;&gt;&lt;/a&gt;解决方法：&lt;/h2&gt;&lt;p style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; margin-top: 0px; margin-bottom: 0px; line-height: 27.132px; padding: 0px; color: rgba(0, 0, 0, 0.870588); font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;1.推荐使用google浏览器、360极速浏览器。&lt;p style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;2.手动改更改页面编码格式为utf-8即可解决。&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;', 1, 7, NULL, 0, 0, 1, 0, 0, 0, 1, 0, '2017-10-28 18:13:56', '2017-10-30 13:24:31', '/upload/news/1710301324256608625.jpg', '未标题-1', '/upload/news/1710301324256608625.jpg', NULL, NULL, NULL, '有时候内容会出现乱码？', '有时候内容会出现乱码？', '问题描述zzzcms全站都是utf-8格式，部分浏览器特别是IE，有些默认使用gbk编码打开页面，提交数据就可能造成数据乱码。解决方法：1.推荐使用google浏览器、360极速浏览器。2.手动改更改页面编码格式为utf-8即可解决。', NULL, NULL, 1, 1);
INSERT INTO zzz_content (cid, c_title, c_lid, c_sid, c_brand, c_type, c_gid, c_exc, c_title2, c_color, c_link, c_tag, c_content, c_onoff, c_order, isoutlink, istop, isgood, ispic, isoffer, issell, ishtml, c_visits, c_star, c_addtime, c_edittime, c_picsurl, c_picsname, c_pic, c_downurl, c_downname, c_pagename, c_pagetitle, c_pagekey, c_pagedesc, zauthor, zsource, zprice, zvprice) VALUES (3, '邮件提醒怎么用？', 1, 6, NULL, 'news', 0, NULL, NULL, '#000000', NULL, NULL, '&lt;h2 data-line=&quot;0&quot; class=&quot;line&quot; style=&quot;box-sizing: inherit; padding: 0px 0px 0.3em; font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; line-height: 1.7em; margin-right: 0px; margin-bottom: 14px; margin-left: 0px; font-weight: 200; font-size: 1.75em; border-bottom-width: 1px; border-bottom-style: solid; border-bottom-color: rgb(238, 238, 238); color: rgba(0, 0, 0, 0.870588); white-space: normal; margin-top: 0px !important; background-color: rgb(255, 255, 255);&quot;&gt;设置邮件提醒?&lt;/h2&gt;&lt;p data-line=&quot;1&quot; class=&quot;line&quot; style=&quot;box-sizing: inherit; margin-top: 0px; margin-bottom: 14px; line-height: 27.132px; padding: 0px; color: rgba(0, 0, 0, 0.870588); font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;zzzcms内置jmail发信功能，&lt;p style=&quot;box-sizing: inherit; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;**位置：**后台-系统管理-系统设置-邮件设置&lt;p style=&quot;box-sizing: inherit; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;**设置信息：**参照默认设置信息，填写您的邮箱。&lt;p style=&quot;box-sizing: inherit; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;&gt; 企业邮箱账号一般是邮件地址，个人邮箱账号一般是@前的账号&lt;/p&gt;&lt;p data-line=&quot;6&quot; class=&quot;line&quot; style=&quot;box-sizing: inherit; margin-top: 0px; margin-bottom: 14px; line-height: 27.132px; padding: 0px; color: rgba(0, 0, 0, 0.870588); font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;**测试邮件：**然后发送测试邮件，如未收到，请确认信息是否正确&lt;p style=&quot;box-sizing: inherit; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;&gt; 测试方法：本地在foxmail等客户端软件上配置，收发信测试&lt;/p&gt;&lt;p data-line=&quot;9&quot; class=&quot;line&quot; style=&quot;box-sizing: inherit; margin-top: 0px; margin-bottom: 14px; line-height: 27.132px; padding: 0px; color: rgba(0, 0, 0, 0.870588); font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;**勾选提醒：**根据需要勾选提醒功能。&lt;/p&gt;&lt;h2 data-line=&quot;12&quot; class=&quot;line&quot; style=&quot;box-sizing: inherit; padding: 0px 0px 0.3em; font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; line-height: 1.7em; margin: 0px 0px 14px; font-weight: 200; font-size: 1.75em; border-bottom-width: 1px; border-bottom-style: solid; border-bottom-color: rgb(238, 238, 238); color: rgba(0, 0, 0, 0.870588); white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&lt;a id=&quot;_12&quot; style=&quot;box-sizing: inherit; color: rgb(65, 131, 196); margin: 0px; padding: 0px; background: transparent;&quot;&gt;&lt;/a&gt;为什么发不出去？&lt;/h2&gt;&lt;p data-line=&quot;13&quot; class=&quot;line&quot; style=&quot;box-sizing: inherit; margin-top: 0px; margin-bottom: 14px; line-height: 27.132px; padding: 0px; color: rgba(0, 0, 0, 0.870588); font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;jmail组件发信，是通过邮件客户端smtp协议，进行发信，属于只发不收，只发不管，并不能得知是否发送成功，由于国内邮件运行商处于网易和腾讯垄断，对新邮箱审核较严格，所以&lt;span style=&quot;box-sizing: inherit; font-weight: 700; margin: 0px; padding: 0px;&quot;&gt;造成很多邮箱不能用于邮件提醒设置&lt;/span&gt;。&lt;/p&gt;&lt;p data-line=&quot;15&quot; class=&quot;line&quot; style=&quot;box-sizing: inherit; margin-top: 0px; margin-bottom: 14px; line-height: 27.132px; padding: 0px; color: rgba(0, 0, 0, 0.870588); font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;1.新注册的个人邮箱，可以说99&#037;都不行。&lt;p style=&quot;box-sizing: inherit; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;2.QQ邮箱，大部分不行。&lt;p style=&quot;box-sizing: inherit; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;3.Gmail等国外邮箱不行。&lt;p style=&quot;box-sizing: inherit; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;4.建议使用1年以上的&lt;span style=&quot;box-sizing: inherit; font-weight: 700; margin: 0px; padding: 0px;&quot;&gt;老个人邮箱&lt;/span&gt;(QQ老邮箱也有不行的)。&lt;p style=&quot;box-sizing: inherit; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;5.建议使用企业邮箱（收费的、免费的都可以）。&lt;p style=&quot;box-sizing: inherit; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;6.建议发件邮箱和收件邮箱用一个邮箱。&lt;p style=&quot;box-sizing: inherit; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;7.建议发件邮箱和手机邮箱用一个品牌（如都是一个品牌的企业邮箱）。&lt;p style=&quot;box-sizing: inherit; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;8.建议让客户使用收费的企业邮箱，有问题有售后，以免后期收发信限制。&lt;/p&gt;&lt;p&gt;&lt;a href=&quot;http://zzzcms.com/case/?27_133.html&quot; style=&quot;box-sizing: inherit; color: rgb(65, 131, 196); text-decoration: none; margin: 0px; padding: 0px; font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; line-height: 27.132px; white-space: normal; background: rgb(255, 255, 255);&quot;&gt;企业邮箱购买地址&lt;/a&gt;&lt;p style=&quot;box-sizing: inherit; margin: 0.2em; padding: 0px; display: block; color: rgba(0, 0, 0, 0.870588); font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; line-height: 27.132px; white-space: normal; background-color: rgb(255, 255, 255);&quot;/&gt;&lt;a href=&quot;http://ym.163.com/&quot; style=&quot;box-sizing: inherit; color: rgb(65, 131, 196); text-decoration: none; margin: 0px; padding: 0px; font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; line-height: 27.132px; white-space: normal; background: rgb(255, 255, 255);&quot;&gt;免费企业邮箱申请&lt;/a&gt;&lt;/p&gt;', 1, 6, NULL, 0, 0, 1, 0, 0, 0, 1, 0, '2017-10-28 18:14:10', '2017-10-30 13:26:37', '/upload/news/1710301326361743741.jpg', 'TIM截图20171030132622', '/upload/news/1710301326361743741.jpg', NULL, NULL, NULL, '邮件提醒怎么用？', '邮件提醒怎么用？', '设置邮件提醒?zzzcms内置jmail发信功能，**位置：**后台-系统管理-系统设置-邮件设置**设置信息：**参照默认设置信息，填写您的邮箱。&gt; 企业邮箱账号一般是邮件地址，个人邮箱账号一般是@前的账号**测试邮件：**然后发送测试邮件，如未收到，请确认信息是否正确&gt; 测试方…', NULL, NULL, 1, 1);
INSERT INTO zzz_content (cid, c_title, c_lid, c_sid, c_brand, c_type, c_gid, c_exc, c_title2, c_color, c_link, c_tag, c_content, c_onoff, c_order, isoutlink, istop, isgood, ispic, isoffer, issell, ishtml, c_visits, c_star, c_addtime, c_edittime, c_picsurl, c_picsname, c_pic, c_downurl, c_downname, c_pagename, c_pagetitle, c_pagekey, c_pagedesc, zauthor, zsource, zprice, zvprice) VALUES (4, '分类为什么不能改成单篇？', 1, 6, NULL, 'news', 0, NULL, NULL, '#000000', NULL, NULL, '&lt;h2 data-line=&quot;0&quot; class=&quot;line&quot; style=&quot;box-sizing: inherit; padding: 0px 0px 0.3em; font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; line-height: 1.7em; margin-right: 0px; margin-bottom: 14px; margin-left: 0px; font-weight: 200; font-size: 1.75em; border-bottom-width: 1px; border-bottom-style: solid; border-bottom-color: rgb(238, 238, 238); color: rgba(0, 0, 0, 0.870588); white-space: normal; margin-top: 0px !important; background-color: rgb(255, 255, 255);&quot;&gt;&lt;span style=&quot;font-size: 15.96px; line-height: 27.132px;&quot;&gt;zzzcms的单篇是独立的数据表，新加分类的时候会自动创建一个单篇表，但编辑不会去创建，所以不能更改为单篇。&lt;/span&gt;&lt;p/&gt;&lt;/h2&gt;&lt;p data-line=&quot;2&quot; class=&quot;line&quot; style=&quot;box-sizing: inherit; margin-top: 0px; margin-bottom: 14px; line-height: 27.132px; padding: 0px; color: rgba(0, 0, 0, 0.870588); font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;如果编辑分类的时候也创建单篇表就会出现，重复大量的单篇表，在前台引用的时候可能会出错，而且会生成大量的单篇。&lt;/p&gt;&lt;p data-line=&quot;5&quot; class=&quot;line&quot; style=&quot;box-sizing: inherit; margin-top: 0px; margin-bottom: 14px; line-height: 27.132px; padding: 0px; color: rgba(0, 0, 0, 0.870588); font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;so:单篇可以更改为其他类型，反之则不能改回单篇。&lt;/p&gt;&lt;blockquote data-line=&quot;7&quot; class=&quot;default line&quot; style=&quot;box-sizing: inherit; margin: 0px; padding: 5px 5px 5px 15px; line-height: 27.132px; color: rgb(119, 119, 119); border-left-width: 4px; border-left-style: solid; border-left-color: rgb(221, 221, 221); font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&lt;p style=&quot;box-sizing: inherit; margin-top: 0px; margin-bottom: 0px; line-height: 1.7em; padding: 0px;&quot;&gt;备注：&lt;p style=&quot;box-sizing: inherit; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;手工修改： 手动打开并修改数据库，zzz_sort和zzz_about表中，手动修改对应好也可以。&lt;/p&gt;&lt;/blockquote&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;', 1, 5, NULL, 1, 0, 0, 0, 0, 0, 2, 0, '2017-10-28 18:14:48', NULL, NULL, NULL, NULL, NULL, NULL, NULL, '分类为什么不能改成单篇？', '分类为什么不能改成单篇？', 'zzzcms的单篇是独立的数据表，新加分类的时候会自动创建一个单篇表，但编辑不会去创建，所以不能更改为单篇。如果编辑分类的时候也创建单篇表就会出现，重复大量的单篇表，在前台引用的时候可能会出错，而且会生成大量的单篇。so:单篇可以更改为其他类型，反之则不能改回单篇…', NULL, NULL, 1, 1);
INSERT INTO zzz_content (cid, c_title, c_lid, c_sid, c_brand, c_type, c_gid, c_exc, c_title2, c_color, c_link, c_tag, c_content, c_onoff, c_order, isoutlink, istop, isgood, ispic, isoffer, issell, ishtml, c_visits, c_star, c_addtime, c_edittime, c_picsurl, c_picsname, c_pic, c_downurl, c_downname, c_pagename, c_pagetitle, c_pagekey, c_pagedesc, zauthor, zsource, zprice, zvprice) VALUES (5, '怎样修改留言提交后返回地址？', 1, 6, NULL, 'news', 0, NULL, NULL, '#000000', NULL, NULL, '&lt;p data-line=&quot;0&quot; class=&quot;line&quot; style=&quot;box-sizing: inherit; margin-bottom: 14px; line-height: 27.132px; padding: 0px; color: rgba(0, 0, 0, 0.870588); font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; white-space: normal; margin-top: 0px !important; background-color: rgb(255, 255, 255);&quot;&gt;文件：&lt;p style=&quot;box-sizing: inherit; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;/inc/zzz_user.asp&lt;p style=&quot;box-sizing: inherit; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;位置：&lt;p style=&quot;box-sizing: inherit; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;459行 461行，将截图中红框内的地址（sitepath&amp;&quot;gbook/?&quot;&amp;gbooksortid&amp;&quot;_1.html&quot;）改为你想要的地址。&lt;/p&gt;&lt;p data-line=&quot;5&quot; class=&quot;line&quot; style=&quot;box-sizing: inherit; margin-top: 0px; margin-bottom: 14px; line-height: 27.132px; padding: 0px; color: rgba(0, 0, 0, 0.870588); font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;返回首页 改为 sitepath&lt;p style=&quot;box-sizing: inherit; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;返回上一页 改为 “-1”&lt;/p&gt;&lt;p data-line=&quot;8&quot; class=&quot;line&quot; style=&quot;box-sizing: inherit; margin-top: 0px; margin-bottom: 0px; line-height: 27.132px; padding: 0px; color: rgba(0, 0, 0, 0.870588); font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&lt;img src=&quot;/upload/news/1710301330136374754.jpg&quot;/&gt;&lt;/p&gt;', 1, 0, NULL, 0, 0, 1, 0, 0, 0, 2, 0, '2017-10-28 18:15:02', '2017-10-30 13:30:21', '/upload/news/1710301330136374754.jpg', NULL, '/upload/news/1710301330136374754.jpg', NULL, NULL, NULL, '怎样修改留言提交后返回地址？', '怎样修改留言提交后返回地址？', '文件：/inc/zzz_user.asp位置：459行 461行，将截图中红框内的地址（sitepath&&quot;gbook/?&quot;&gbooksortid&&quot;_1.html&quot;）改为你想要的地址。返回首页 改为 sitepath返回上一页 改为 “-1”', NULL, NULL, 1, 1);
INSERT INTO zzz_content (cid, c_title, c_lid, c_sid, c_brand, c_type, c_gid, c_exc, c_title2, c_color, c_link, c_tag, c_content, c_onoff, c_order, isoutlink, istop, isgood, ispic, isoffer, issell, ishtml, c_visits, c_star, c_addtime, c_edittime, c_picsurl, c_picsname, c_pic, c_downurl, c_downname, c_pagename, c_pagetitle, c_pagekey, c_pagedesc, zauthor, zsource, zprice, zvprice) VALUES (6, '后台管理账号经常掉线，经常需要输入密码', 1, 6, NULL, 'news', 0, NULL, NULL, '#000000', NULL, NULL, '&lt;h2 data-line=&quot;0&quot; class=&quot;line&quot; style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; padding: 0px 0px 0.3em; font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; line-height: 1.7em; margin: 0px 0px 14px; font-weight: 200; font-size: 1.75em; border-bottom-width: 1px; border-bottom-style: solid; border-bottom-color: rgb(238, 238, 238); color: rgba(0, 0, 0, 0.870588); white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;问题描述：&lt;/h2&gt;&lt;p style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; margin-top: 0px; margin-bottom: 14px; line-height: 27.132px; padding: 0px; color: rgba(0, 0, 0, 0.870588); font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;一般是香港空间，国外空间，小空间，由于网络不稳定或服务器资源较少，造成的session丢失，所以就会发生几分钟就掉线的情况。&lt;/p&gt;&lt;h2 data-line=&quot;3&quot; class=&quot;line&quot; style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; padding: 0px 0px 0.3em; font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; line-height: 1.7em; margin: 0px 0px 14px; font-weight: 200; font-size: 1.75em; border-bottom-width: 1px; border-bottom-style: solid; border-bottom-color: rgb(238, 238, 238); color: rgba(0, 0, 0, 0.870588); white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&lt;a id=&quot;_3&quot; style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; color: rgb(65, 131, 196); margin: 0px; padding: 0px; background: transparent;&quot;&gt;&lt;/a&gt;解决方法：&lt;/h2&gt;&lt;p style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; margin-top: 0px; margin-bottom: 14px; line-height: 27.132px; padding: 0px; color: rgba(0, 0, 0, 0.870588); font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;1.自然是更换国内的备案空间，推荐阿里云空间（淘宝上有卖，只需要50元），永久解决此问题。&lt;p style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;2.更改session服务器验证，为cookie本地验证模式，修改config/zzz_config.asp，&lt;p style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;&lt;code style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; font-family: Consolas, &#39;Liberation Mono&#39;, Menlo, Courier, monospace; font-size: 1em; margin: 0px; padding: 0px 0.4em; display: inline-block; border-radius: 4px; word-break: break-all; white-space: pre; line-height: 1.3; background-color: rgb(247, 247, 247);&quot;&gt;Const issession=0&lt;/code&gt;&lt;p style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;改为&lt;p style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;&lt;code style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; font-family: Consolas, &#39;Liberation Mono&#39;, Menlo, Courier, monospace; font-size: 1em; margin: 0px; padding: 0px 0.4em; display: inline-block; border-radius: 4px; word-break: break-all; white-space: pre; line-height: 1.3; background-color: rgb(247, 247, 247);&quot;&gt;Const issession=1&lt;/code&gt;&lt;p style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;优点是不受服务器和网络影响，缺点是有cookie欺骗危险，不适合数据安全有要求的客户。&lt;p style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;session默认失效时间一般为20分钟，cookie一般是浏览器才失效。&lt;/p&gt;&lt;p style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; margin-top: 0px; margin-bottom: 0px; line-height: 27.132px; padding: 0px; color: rgba(0, 0, 0, 0.870588); font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&lt;img src=&quot;/upload/news/171028/2017102818160380.png&quot; alt=&quot;&quot; style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; border: 0px; margin: 0px; padding: 0px; max-width: 100&#037;;&quot;/&gt;&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;', 1, 3, NULL, 0, 1, 1, 0, 0, 0, 1, 0, '2017-10-28 18:16:11', '2017-10-30 13:28:24', '/upload/news/171028/2017102818160518.png', '2017102818160518', '/upload/news/171028/2017102818160518.png', NULL, NULL, NULL, '后台管理账号经常掉线，经常需要输入密码', '后台管理账号经常掉线|经常需要输入密码', '问题描述：一般是香港空间，国外空间，小空间，由于网络不稳定或服务器资源较少，造成的session丢失，所以就会发生几分钟就掉线的情况。解决方法：1.自然是更换国内的备案空间，推荐阿里云空间（淘宝上有卖，只需要50元），永久解决此问题。2.更改session服务器验证，为cooki…', NULL, NULL, 1, 1);
INSERT INTO zzz_content (cid, c_title, c_lid, c_sid, c_brand, c_type, c_gid, c_exc, c_title2, c_color, c_link, c_tag, c_content, c_onoff, c_order, isoutlink, istop, isgood, ispic, isoffer, issell, ishtml, c_visits, c_star, c_addtime, c_edittime, c_picsurl, c_picsname, c_pic, c_downurl, c_downname, c_pagename, c_pagetitle, c_pagekey, c_pagedesc, zauthor, zsource, zprice, zvprice) VALUES (7, 'sql server数据库的备份和还原', 1, 6, NULL, 'news', 0, NULL, NULL, '#000000', NULL, NULL, '&lt;h2 data-line=&quot;0&quot; class=&quot;line&quot; style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; padding: 0px 0px 0.3em; font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; line-height: 1.7em; margin: 0px 0px 14px; font-weight: 200; font-size: 1.75em; border-bottom-width: 1px; border-bottom-style: solid; border-bottom-color: rgb(238, 238, 238); color: rgba(0, 0, 0, 0.870588); white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;zzzcms数据库备份还原&lt;/h2&gt;&lt;p style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; margin-top: 0px; margin-bottom: 14px; line-height: 27.132px; padding: 0px; color: rgba(0, 0, 0, 0.870588); font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;asp语言的cms基本上都放弃了mssql数据库的备份和还原功能，zzzcms不负众望终于解决了这个大问题，只要对用户建站有一些帮助就不负作者努力。&lt;/p&gt;&lt;hr style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; height: 0px; margin: 0px 0px 0.5em; padding: 0px; border-top-width: 0px; border-right-width: 0px; border-left-width: 0px; border-bottom-style: solid; border-bottom-color: rgb(221, 221, 221); color: rgba(0, 0, 0, 0.870588); font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; line-height: 27.132px; white-space: normal; background-color: rgb(255, 255, 255);&quot;/&gt;&lt;p style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; margin-top: 0px; margin-bottom: 14px; line-height: 27.132px; padding: 0px; color: rgba(0, 0, 0, 0.870588); font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; font-size: 15.96px; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;经测试发现，基本上都支持mssql的数据备份，但部分的空间不支持还原。&lt;p style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; margin: 0.2em; padding: 0px; display: block;&quot;/&gt;所以zzzcms在mssql环境下，针对虚拟空间环境关闭了数据还原功能，下面有开启方法供修改参照。&lt;/p&gt;&lt;h2 data-line=&quot;10&quot; class=&quot;line&quot; style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; padding: 0px 0px 0.3em; font-family: &#39;Microsoft Yahei&#39;, &#39;Helvetica Neue&#39;, Arial, Helvetica, sans-serif; line-height: 1.7em; margin: 0px 0px 14px; font-weight: 200; font-size: 1.75em; border-bottom-width: 1px; border-bottom-style: solid; border-bottom-color: rgb(238, 238, 238); color: rgba(0, 0, 0, 0.870588); white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&lt;a id=&quot;_10&quot; style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; color: rgb(65, 131, 196); margin: 0px; padding: 0px; background: transparent;&quot;&gt;&lt;/a&gt;平台测试结果：&lt;/h2&gt;&lt;table&gt;&lt;thead style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; margin: 0px; padding: 0px;&quot;&gt;&lt;tr style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; margin: 0px; padding: 0px;&quot; class=&quot;firstRow&quot;&gt;&lt;th style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; padding: 3px 12px; margin: 0px; border-color: rgb(136, 136, 136);&quot;&gt;平台&lt;/th&gt;&lt;th style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; padding: 3px 12px; margin: 0px; border-color: rgb(136, 136, 136);&quot;&gt;备份&lt;/th&gt;&lt;th style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; padding: 3px 12px; margin: 0px; border-color: rgb(136, 136, 136);&quot;&gt;还原&lt;/th&gt;&lt;th style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; padding: 3px 12px; margin: 0px; border-color: rgb(136, 136, 136);&quot;&gt;问题&lt;/th&gt;&lt;/tr&gt;&lt;/thead&gt;&lt;tbody style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; margin: 0px; padding: 0px;&quot;&gt;&lt;tr style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; margin: 0px; padding: 0px;&quot;&gt;&lt;td style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; padding: 3px 12px; margin: 0px; border-color: rgb(136, 136, 136); border-top-left-radius: 3px;&quot;&gt;阿里云基础版&lt;/td&gt;&lt;td style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; padding: 3px 12px; margin: 0px; border-color: rgb(136, 136, 136);&quot;&gt;支持&lt;/td&gt;&lt;td style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; padding: 3px 12px; margin: 0px; border-color: rgb(136, 136, 136);&quot;&gt;不支持&lt;/td&gt;&lt;td style=&quot;box-sizing: inherit; -webkit-tap-highlight-color: transparent; -webkit-font-smoothing: antialiased; padding: 3px 12px; margin: 0px; border-color: rgb(136, 136, 136); border-top-right-radius: 3px;&quot;&gt;权限不足，速度较快&l