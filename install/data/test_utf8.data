INSERT INTO zzz_content (cid, c_title, c_lid, c_sid, c_brand, c_type, c_gid, c_exc, c_title2, c_color, c_link, c_tag, c_content, c_onoff, c_order, isoutlink, istop, isgood, ispic, isoffer, issell, ishtml, c_visits, c_star, c_addtime, c_edittime, c_picsurl, c_picsname, c_pic, c_downurl, c_downname, c_pagename, c_pagetitle, c_pagekey, c_pagedesc, zauthor, zsource, zprice, zvprice) VALUES (20, '第一节：初识zzzcms，这是一个视频演示', 1, 19, NULL, 'video', 0, NULL, NULL, '#000000', NULL, NULL, '&lt;p&gt;&lt;iframe src=&quot;http://player.youku.com/embed/XMjYxNjk5MjU0MA==&quot; width=&quot;800&quot; height=&quot;498&quot; scrolling=&quot;no&quot; frameborder=&quot;0&quot; align=&quot;&quot;&gt;&lt;/iframe&gt;&lt;/p&gt;', 1, 9, NULL, 0, 0, 0, 0, 0, 0, 3, 0, '2017-10-30 13:48:56', '2017-10-30 13:50:29', NULL, NULL, NULL, NULL, NULL, NULL, '第一节：初识zzzcms，这是一个视频演示', '第一节：初识zzzcms|这是一个视频演示', NULL, NULL, NULL, 1, 1);
INSERT INTO zzz_content (cid, c_title, c_lid, c_sid, c_brand, c_type, c_gid, c_exc, c_title2, c_color, c_link, c_tag, c_content, c_onoff, c_order, isoutlink, istop, isgood, ispic, isoffer, issell, ishtml, c_visits, c_star, c_addtime, c_edittime, c_picsurl, c_picsname, c_pic, c_downurl, c_downname, c_pagename, c_pagetitle, c_pagekey, c_pagedesc, zauthor, zsource, zprice, zvprice) VALUES (21, 'aspcms转zzzcms工具 V1.5 Build20170719', 1, 7, NULL, 'news', 0, NULL, NULL, '#000000', NULL, NULL, '&lt;p&gt;aspcms程序迁入zzzcms1.5正式版&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;Build20170719&lt;/p&gt;&lt;p&gt;修复导入数据后产品修改报错的bug。&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;Build20170406&lt;/p&gt;&lt;p&gt;支持aspcms2.2以上版本&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;***********************************************&lt;/p&gt;&lt;p&gt;Build20170325&lt;/p&gt;&lt;p&gt;修复部分bug。&lt;/p&gt;&lt;p&gt;增加对模板中自定义参数标签的替换&lt;/p&gt;&lt;p&gt;如[content:C_riqi]替换为[content:zriqi]&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;aspcms程序迁入zzzcms1.2正式版&lt;/p&gt;&lt;p&gt;***********************************************&lt;/p&gt;&lt;p&gt;Build20170309&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;1.2更新说明：&lt;/p&gt;&lt;p&gt;1.修复模板中既有gbk还有utf模板乱码的问题。&lt;/p&gt;&lt;p&gt;2.支持替换数据库中的内链，自动匹配为新地址。&lt;/p&gt;&lt;p&gt;3.支持更多模板标签。&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;***********************************************&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;使用说明：&lt;/p&gt;&lt;p&gt;1.下载最新版zzzcms程序，解压。&lt;/p&gt;&lt;p&gt;下载地址:http://demo.zzzcms.com/zzzcms.zip&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;2.解压工具，到网站zzzcms根目录，并设置为iis默认路径，&lt;/p&gt;&lt;p&gt;打开http://localhost/admin/，进入后台初始化安装。&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;3.将aspcms的数据库复制到zzzcms程序的根目录。&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;4.　复制aspcms/templates/???/&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;　粘贴到zzzcms/template/pc/目录中。&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;（???是你的模板名称，如cn2014）&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;5.打开 http://localhost/aspcms.asp&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;6.输入数据库名称，提交迁移数据库，迁移过程中不可做其他操作。&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;7.打开 http://localhost/aspcmshtml.asp&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;8.写入模板路径，如cn2014,html路径，提交开始替换模板，替换过程中不可做其他操作。&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;9.浏览 http://localhost/&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;10.对模板进行必要的修改，一般模板中使用了固定链接的需要手动修改链接地址。&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;11.把aspcms程序中的，upload,images,js,css 复制到zzzcms根目录，正常情况下 网站迁移就完成了。&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;-------------------------------------------------------------------------------------------------&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;注意事项：&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;1.运行本程序后，zzzcms的数据将清空，一定不要用有数据的zzzcms程序做导入。&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;2.一定要先迁移数据库，在迁移模板。&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;-------------------------------------------------------------------------------------------------&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;功能介绍：&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;1.支持分类，单篇，内容，自定义参数，友情链接，自定义标签&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;2.不支持订单，广告&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;&lt;p&gt;3.迁入后，分类id和内容id都将初始化排序，不过不影响网站访问，已重做了关联。&lt;/p&gt;&lt;p&gt;&lt;p/&gt;&lt;/p&gt;', 1, 9, NULL, 0, 0, 0, 0, 0, 0, 1, 0, '2017-10-30 13:52:54', '2017-10-30 14:51:13', NULL, NULL, NULL, '/upload/down/aspcmschengxuqianruzzzcmsshiyongbangzhu.txt, /upload/down/aspcmszhuazzzcmsgongjuV15Build20170719.zip,', '使用帮助, 下载工具,', NULL, 'aspcms转zzzcms工具 V1.5 Build20170719', 'aspcms转zzzcms工具|V1.5|Build20170719', 'aspcms程序迁入zzzcms1.5正式版Build20170719修复导入数据后产品修改报错的bug。Build20170406支持aspcms2.2以上版本***********************************************Build20170325修复部分bug。增加对模板中自定义参数标签的替换如[content:C_riqi]替换为[content:zriqi]as…', NULL, NULL, 1, 1);
